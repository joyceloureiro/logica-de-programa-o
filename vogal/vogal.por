programa {
  funcao inicio() 
  {
    caracter letra_digitada
	   
		escreva("informe uma letra")
		leia(letra_digitada)

		escolha (letra_digitada)
          {
		caso 'a' :
		     escreva ("\n é vogal")
		pare

		caso 'e' :
		     escreva ("\n é vogal")
		pare

		caso 'i' :
		     escreva ("\n é vogal")
		pare

		caso 'o' :
		     escreva ("\n é vogal")
		pare

		caso 'u' :
		     escreva ("\n é vogal")
		pare

		caso contrario:
		     escreva ("\n é consoante")

          }
	}
}
    
  }
}
